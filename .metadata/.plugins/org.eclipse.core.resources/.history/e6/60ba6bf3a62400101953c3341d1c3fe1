package gdtest;

public class Q24 {
	public static void main(String[] args) {
		A a = new B();
		a.paint();
		a.draw();
	}
}

class A {
	public void paint() {
		System.out.print("A");
		draw(); 
	}
	
	public void draw() {
		System.out.print("B"); 
		draw(); 
	}
}

class B extends A {
	public void paint() {
		super.draw();
		System.out.print("C"); 
	}
	
	public void draw() {
		System.out.print("D"); 
	}
}
