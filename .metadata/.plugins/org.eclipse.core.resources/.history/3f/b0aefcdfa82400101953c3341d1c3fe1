package gdtest;

import java.util.ArrayList;
import java.util.Collections;
import java.util.HashSet;
import java.util.List;
import java.util.Set;

public class Q26 {
	
	public static void main(String[] args) {
		String[] arr = { "1", "2", "9", "7", "4", "6", "1", "7", "0" };
		Set<String> set = new HashSet<>(); // 중복값 X "1", "2", "9", "7", "4", "6", "1", "7", "0"
		for(String s : arr) {
			set.add(s);
		}
		
		List<String> list = new ArrayList<>(set); // set -> list : 순서가 필요
		// 오름차순 정렬
		Collections.sort(list); // Math 클래스 : static 수학 메서드, Collections : static collection 핸들링 메서드
  
		for(String s : list) { 
			System.out.println(s); 
		}
	 
		/*
			정답
			0
			1
			2
			4
			6
			7
			9
		*/ 
	}
}
