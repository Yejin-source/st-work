package gdtest;

public class Q04 {
	
	public static void main(String[] args) {
		
		/*
	  	다음 중 Java에서 부정(논리 NOT) 연산자는 무엇입니까?
		1번보기   &&   
		2번보기   ||   
		3번보기   !   
		4번보기   &
		 */
		
		int x = 1;
		int y = 1;
		System.out.println(x != 1 && ++x == 2);
		System.out.println(y != 1 & ++y == 2);
		
	}
}
