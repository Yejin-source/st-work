package gdtest;

public class Q21 {
	
	public static void main(String[] args) {
		
		  int x = 3, result = 1;
		  
		  switch(x) {
			  case 1: 
				  result++;
			  case 2: 
				  result -= 3;
			  case 3: 
				  result = 0;
			  case 4: 
				  result += 3;
			  case 5: 
				  result -= 10;
			  default: 
				  result--;
		  }
		  
		  // 실행결과
		  System.out.println(result);
		  
		  
		  // brake; 가 없기 때문에 참일 때부터 마지막까지 실행됨 
	}
}
