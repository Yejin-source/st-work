package gdtest;

public class Q24 {
	public static void main(String[] args) {
		A a = new B(); // a는 A 타입이지만, 실제로는 B 객체를 가리키고 있음 -> 메서드를 호출할 때 B를 찾아감
		a.paint(); // B의 paint() 호출
		a.draw();
	}
}

class A {
	public void paint() {
		System.out.print("A");
		draw();
	}
	
	public void draw() { // 1. 부모 클래스(A)의 draw 메서드 실행
		System.out.print("B"); // 1. 출력
		draw(); // this.draw(); -> B의 draw() 호출
	}
}

class B extends A {
	public void paint() {
		super.draw(); // 1. 실행하면 (부모 클래스의 메서드 호출)
		System.out.print("C"); 
	}
	
	public void draw() {
		System.out.print("D"); 
	}
}


// 정답 BDCD


// super가 아니면 전부 this
